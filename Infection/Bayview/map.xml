<map proto="1.4.0" game="Infection">
<name>Bayview</name>
<version>1.1.1</version>
<objective>Survive the zombie army</objective>
<authors>
    <author uuid="27ac2daf-3c38-4b8e-9448-46f221bb612a"/> <!-- n11ck -->
    <author uuid="39a9e1b5-1e1d-40c0-8c16-69a9568efa72"/> <!-- Teejers -->
    <author uuid="c4ad59fd-a7c6-42f1-8a5f-5af0aa60353b"/> <!-- Reddemptr -->
</authors>
<include src="infection.xml"/>
<spawns>
    <spawn filter="before-10s" team="zombie-team" kit="zombie-kit-start">
        <regions yaw="0">
            <point>-66,13.5,-42</point>
        </regions>
    </spawn>
    <spawn filter="before-10s" team="human-team" kit="human-kit">
        <regions yaw="0">
            <point>44,13.5,-78</point>
        </regions>
    </spawn>
    <spawn filter="after-10s" team="zombie-team" kit="zombie-kit">
        <regions yaw="0">
            <point>66,13.5,-42</point>
        </regions>
    </spawn>
    <spawn filter="after-10s" team="human-team" kit="zombie-switch-kit">
        <region yaw="0">
            <point>44,13.5,-78</point>
        </region>
    </spawn>
    <default>
        <regions yaw="0">
            <point>-2,17.5,-60</point>
        </regions>
    </default>
</spawns>
<filters>
    <not id="deny-void">
        <void/>
    </not>
    <not id="deny-ender-chest">
        <material>ender chest</material>
    </not>
</filters>
<regions>
    <above id="height-limit" y="255"/>
    <apply block="never" message="You may not modify this area!">
        <region>
            <cuboid min="0,0,0" max="10000,100,1000"/>
        </region>
    </apply>
    <apply block="deny-void" message="You may not build outside of the playable region!"/>
    <apply use="deny-ender-chest"/>
</regions>
<itemremove>
    <item>obsidian</item>
</itemremove>
</map>
